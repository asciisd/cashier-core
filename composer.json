{
    "name": "asciisd/cashier-core",
    "description": "A flexible payment processing system for Laravel using Factory Pattern",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "payment",
        "cashier",
        "factory-pattern",
        "payment-processing"
    ],
    "authors": [
        {
            "name": "ASCII SD",
            "email": "info@asciisd.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "pestphp/pest": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Asciisd\\CashierCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciisd\\CashierCore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asciisd\\CashierCore\\CashierCoreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
